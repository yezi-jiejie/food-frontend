{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/管理员",
      "dest": "/管理员.html"
    },
    {
      "src": "/功能页", 
      "dest": "/功能页.html"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg))",
      "dest": "/$1"
    }
  ]
}
